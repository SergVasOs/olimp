{
  "name": "olimp",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-mobile": "OLIMP_VIEW=mobile rm -R -f ./dist && cp -r ./public ./dist && export NODE_ENV=production && webpack --mode=production",
    "start-mobile": "OLIMP_VIEW=mobile webpack serve --mode=development",
    "build-desktop": "OLIMP_VIEW=desktop rm -R -f ./dist && cp -r ./public ./dist && export NODE_ENV=production && webpack --mode=production",
    "start-desktop": "OLIMP_VIEW=desktop webpack serve --mode=development",
    "pretty": "prettier --write '**/*.pug'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prettier/plugin-pug": "^1.14.0",
    "@tailwindcss/line-clamp": "^0.2.0",
    "autoprefixer": "^10.2.5",
    "css-loader": "^5.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.10",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "pug": "^3.0.2",
    "pug-loader": "github:antpaw/pug-loader#master",
    "sass-loader": "^11.0.1",
    "tailwindcss": "^2.1.1",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
