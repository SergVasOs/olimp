include /include/blocks/common/pageHead/pageHead

mixin catalogHead(title='Кубки пластиковые с длинным названием')
	+pageHead(title, 'catalog.html')&attributes(attributes)
		+e.right
			+b.button._icon._lightGray(href='#popup_categories')
				+e.icon
					include /assets/images/lg/categoriesIcon.svg
			+b.button._icon._yellow(href='#popup_filters')
				+e.icon
					include /assets/images/lg/filterIcon1.svg
