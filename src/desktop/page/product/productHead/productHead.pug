include /include/elements/favoriteIcon/favoriteIcon

mixin productHead
	+b.productHead&attributes(attributes)
		+e.box.box
			+e.title Стеклянная награда J42-115 с цветным нанесением, высота 11.5 см
			+b.icon._lg.__shareIcon(href='#')
				include /assets/images/md/shareIcon.svg
			+favoriteIcon._active
